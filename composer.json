{
    "name": "maxime-rainville/silverstripe-enhanced-textfield-tutorial",
    "description": "A simple tutorial demo",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "MaximeRainville\\EnhancedTextfield\\": "src/",
            "MaximeRainville\\EnhancedTextfield\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "expose": [
            "client/dist",
            "client/lang",
            "thirdparty"
        ]
    }
}
